            "representative_amount": null,
            "item_type": "pos_rebate",
            "owner_status": [
                "homeowner"
            ],
            "ami_qualification": "less_than_80_ami",
            "agi_max_limit": null,
            "filing_status": null,
            "start_date": 2023,
            "end_date": 2032,
            "eligible": true
        }
